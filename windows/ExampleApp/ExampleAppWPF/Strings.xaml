    <system:String x:Key="OptionsViewReplayTitle">Replay Tutorials</system:String>
    <system:String x:Key="OptionsViewReplayMessage">The help panels will be visible again when you enter or leave a building.</system:String>

    <system:String x:Key="ClearCacheSubViewTitle">Remove Stored Data</system:String>
    <system:String x:Key="ClearCacheSubViewConfirmMessageBig">Warning!</system:String>
    <system:String x:Key="ClearCacheSubViewConfirmMessageSmall">Are you sure you want to remove all stored data?</system:String>
    <system:String x:Key="ClearCacheSubViewWorkingMessageSmall">Please wait, this may take a while...</system:String>
    <system:String x:Key="ClearCacheSubViewDoneMessageSmall">Map data deleted from device.</system:String>

</ResourceDictionary>
