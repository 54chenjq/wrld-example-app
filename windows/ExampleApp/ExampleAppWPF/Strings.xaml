<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:ExampleAppWPF">

    <system:String x:Key="AboutViewTitleText">About</system:String>
    <system:String x:Key="AboutViewDevelopersText">Developed by Bloomberg L.P and:</system:String>

    <system:String x:Key="OptionsViewTitleText">Options</system:String>
    <system:String x:Key="OptionsViewCacheEnabledText">Enable data caching on device</system:String>
    <system:String x:Key="OptionsViewClearCacheText">Clear cached map data</system:String>
    <system:String x:Key="OptionsViewPlayTutorialText">Play tutorial again</system:String>
